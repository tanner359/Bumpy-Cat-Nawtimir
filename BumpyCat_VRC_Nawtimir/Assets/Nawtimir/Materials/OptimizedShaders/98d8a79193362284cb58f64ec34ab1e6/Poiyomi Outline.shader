					poiMods.audioLink[1] = (0.0 /*_AudioLinkSmoothingLowMid*/)  == 0 ? AudioLinkData(ALPASS_AUDIOLINK + float2(0, 1))[0] : AudioLinkData(ALPASS_FILTEREDAUDIOLINK + float2((1 - (0.0 /*_AudioLinkSmoothingLowMid*/) ) * 15.95, 1))[0];
